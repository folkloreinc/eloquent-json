{
    "name": "folklore/eloquent-json",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/Folkloreatelier",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@atelierfolklore.ca",
            "homepage": "http://atelierfolklore.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
        "guzzlehttp/guzzle": "5.3|~6.0|^6.3",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/database": "3.5.*|3.6.*|3.7.*|3.8.*|^4.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|^4.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0|~7.0|^7.5|^8.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-0": {
            "Folklore\\EloquentJson": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Folklore\\EloquentJson\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/fixture"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\EloquentJson\\JsonServiceProvider"
            ]
        }
    }
}
